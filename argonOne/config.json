{
   "name": "VirageHub Active Cooling",
   "version": "0.3",
   "slug": "argon_one_temp",
   "description": "Actively cool your VirageHub.",
   "arch": ["armhf", "armv7", "aarch64"],
   "startup": "application",
   "full_access": "no",
   "boot": "auto",
   "auth_api": true,
   "homeassistant_api": true,
   "hassio_role": "homeassistant",
   "hassio_api": true,
   "devices": ["/dev/i2c-1"],
   "host_network": false,
   "url":"https://github.com/brandondb1/HassOSArgonOneAddon",
   "map": ["config", "ssl", "addons", "backup", "share", "media"],
   "options": {
      "CorF": "C",
      "LowRange": 40,
      "MediumRange": 60,
      "HighRange": 70,
      "QuietProfile": false,
      "Create a Fan Speed and Temperature entity": true
   },
   "schema": {
      "CorF": "str",
      "LowRange": "int(0,255)",
      "MediumRange": "int(0,255)",
      "HighRange": "int(0,255)",
      "QuietProfile": "bool",
      "Create a Fan Speed and Temperature entity": "bool"
   }
}
